<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:p="http://www.springframework.org/schema/p"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans 
http://www.springframework.org/schema/beans/spring-beans.xsd"
	   default-lazy-init="true">
	<bean id="simpleBean1"
		  class="my.home.pro.SimpleBean"
		  init-method="init" p:name="Chris Schaefer" p:age="32"/>
 
	<bean id="simpleBean2"
		  class="my.home.pro.SimpleBean"
		  init-method="init" p:age="32"/>
 
	<bean id="simpleBean3"
		  class="my.home.pro.SimpleBean"
		  init-method="init" p:name="Chris Schaefer"/> 

	<bean id="destructiveBean" class="my.home.pro.DestructiveBean"
		  p:filePath="#{systemProperties['java.io.tmpdir']}#{systemProperties['file.separator']}test.txt" 
		  destroy-method="destroy"/>

	<bean id="dWithInterface" class="my.home.pro.DestructiveBeanWithInterface"/>
	<bean id="beanNamePrinter" class="my.home.pro.BeanNamePrinter"/>

</beans>
